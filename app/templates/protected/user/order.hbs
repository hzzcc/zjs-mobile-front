{{#if content.length}}

  <ul class="nav-ver-list order-list">
    <li class="order-list-tit">
      <span class="wp1 bold">类型</span>
      <span class="wp2 bold center">金额（元）</span>
      <span class="wp3 bold center">日期</span>
      <span class="wp4 bold right">收益（元）</span>
    </li>
    {{#each content as |order| }}
      <li class="nav-ver-item">
        <div class="item-content">
          <span class="wp1 {{order.typeColor}}">{{order.order_type}}</span>
          <span class="wp2 center">{{order.total_pay}}</span>
          <span class="wp3 center">{{order.created_time}}</span>
          <span class="wp4 center">{{order.earning}}</span>
        </div>
      </li>
    {{/each}}
  </ul>

  <!-- {{page-numbers content=content class="pull-right" dots=true}} -->
{{/if}}
